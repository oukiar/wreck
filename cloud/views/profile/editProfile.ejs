.click();});$("#inputPhoto").change(function(){	$('#preview_crop').dialog({modal:true, width:230});  		/*Enviar objectID en un campo hidden*/		$("#frm_uploadPhoto").submit();});$("#inputPhoto_disabled").change(	function()	{				if (this.files && this.files[0]) {            var reader = new FileReader();                      reader.onload = function (e) {                $('#blah').attr('src', e.target.result);                                $('#preview_crop').dialog({modal:true, width:400});                								                $('#cropimage').cropper({									  aspectRatio: 16 / 9,									  autoCropArea: 0.65,									  strict: false,									  guides: false,									  highlight: false,									  dragCrop: false,									  movable: false,									  resizable: false									});            };                        reader.readAsDataURL(this.files[0]);                  }        			});/*$( "#draggable" ).draggable();$( "#draggable" ).resizable();*/</script>