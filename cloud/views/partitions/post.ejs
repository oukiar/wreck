		<% posts.forEach(function(post) { %><div class='news_cont'>	<div class='status_news'>	<div class='title_news'><h3><%= post.get("title") %></h3> </div>	<div class='date_news'>Date	</div>    </div>			<div class='post_subHeader'>	<span style="position:relative; float:right;">Global</span>	<img style="position:relative; float:right; top:0px;" src="web/icons/location.png" width="21" height="21" alt="Location">	<span id="postCategory" style="position:relative; top:5px;">Politics</span>	<img style="float:left;" src="web/icons/favorite.png" width="21" height="21" alt="Favorite" id="<%= post.id%>" onclick="favorite(this.id, '<%= wreckUserId %>')">	</div>	   	<div class='cont_news' >		<p><%= post.get("postContent") %></p>	</div>			<div class='post_media'>		<img style="float:left; margin-right:5px;" src="web/imgs/thumbs/1.jpg" width="89" height="89" alt="1">				<img style="position:relative; float:right; margin-right:5px; top:21px;" src="web/icons/videomedia.png" width="55" height="55" alt="Videomedia">	    </div>				<div class='post_news'>	<img style="float:left;" src="web/icons/profile.png"  height="55">		<span style="position:relative; top: 21px;float:left;">$posterName $posterLastName</br>Follow</span>		<span style="position:relative; top:8px; float:right;">		<img src="web/icons/facebook.png" width="34" height="34" alt="Facebook" >		<img src="web/icons/twitter.png" width="34" height="34" alt="Twitter">		<img src="web/icons/share.png" width="34" height="34" alt="Share">		<img src="web/icons/comments.png" width="34" height="34" alt="Comments">		<span id="Comments">108</span>		<img id="upVote" src="web/icons/upVote.png" width="34" height="34" alt="Vote Up" onclick="Vote()"><span id="upPoints"></span>	<img id="downVote" src="web/icons/downVote.png" width="34" height="34" alt="Vote Down" onclick="downVote()"></span></div>			<div class='post_tags'>	<span id="postTags">Tags:</span>	</div></div> <div class='line_sepnews'></div> <% }) %>	